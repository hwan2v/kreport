{
  "settings": {
    "index": {
      "max_ngram_diff": "100",
      "analysis": {
        "tokenizer": {
          "edge_ngram_tokenizer": {
            "type": "edge_ngram",
            "min_gram": 1,
            "max_gram": 60,
            "token_chars": [
              "letter",
              "digit"
            ]
          },
          "ngram_tokenizer": {
            "type": "ngram",
            "min_gram": 2,
            "max_gram": 60,
            "token_chars": [
              "letter",
              "digit"
            ]
          }
        },
        "filter": {
          "edge_ngram_filter": {
            "type": "edge_ngram",
            "min_gram": 1,
            "max_gram": 20
          }
        },
        "char_filter": {
          "remove_whitespace": {
            "type": "pattern_replace",
            "pattern": "\\s+",
            "replacement": ""
          }
        },
        "analyzer": {
          "edge_ngram_analyzer": {
            "type": "custom",
            "tokenizer": "edge_ngram_tokenizer",
            "filter": [
              "lowercase"
            ]
          },
          "no_whitespace_edge_ngram_analyzer": {
            "type": "custom",
            "char_filter": [
              "remove_whitespace"
            ],
            "tokenizer": "edge_ngram_tokenizer",
            "filter": [
              "lowercase"
            ]
          },
          "ngram_analyzer": {
            "type": "custom",
            "tokenizer": "ngram_tokenizer",
            "filter": [
              "lowercase"
            ]
          },
          "search_analyzer": {
            "type": "custom",
            "tokenizer": "standard",
            "filter": [
              "lowercase"
            ]
          }
        }
      }
    }
  },
